<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginlogout.HomeController">
   <children>
      <Label layoutX="62.0" layoutY="62.0" text="WELCOME">
         <font>
            <Font name="Arial Black" size="31.0" />
         </font>
      </Label>
      <Label fx:id="headerText" layoutX="117.0" layoutY="118.0" text="" textAlignment="CENTER" wrapText="true" >
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </Label>
       <Button fx:id="deleteButton" layoutX="32.0" layoutY="425.0" onAction="#handleDeleteAction" prefHeight="42.0" prefWidth="236.0" styleClass="btn" stylesheets="@style.css" text="DELETE USER ACCOUNT" />
      <Button fx:id="logoutButton" layoutX="32.0" layoutY="363.0" onAction="#handleLogoutAction" prefHeight="42.0" prefWidth="236.0" styleClass="btn" stylesheets="@style.css" text="LOGOUT" />
      <Button fx:id="editButton" layoutX="32.0" layoutY="303.0" onAction="#handleEditAction" prefHeight="42.0" prefWidth="236.0" styleClass="btn" stylesheets="@style.css" text="EDIT USER ACCOUNT" />
   </children>
</AnchorPane>
