      <Button fx:id="editButton" layoutX="32.0" layoutY="303.0" onAction="#handleLogoutAction" prefHeight="42.0" prefWidth="236.0" styleClass="btn" stylesheets="@style.css" text="EDIT USER ACCOUNT" />
